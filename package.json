{
  "name": "micro-shop-products",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "start": "npm run createDb && npm run migrate && nodemon --experimental-specifier-resolution=node dist/app/express/index.js",
    "start:dev": "nodemon --experimental-specifier-resolution=node --exec ts-node -r tsconfig-paths/register --esm ./src/app/express/index.ts -e ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "createDb": "npx ts-node --esm ./src/database/createDB.ts",
    "migrate": "npx ts-node --esm ./node_modules/.bin/knex migrate:latest --knexfile ./src/database/knexfile.ts",
    "@knex": "./node_modules/.bin/knex --knexfile ./src/database/knexfile.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amitsatpute-pyjs/micro-shop-products.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amitsatpute-pyjs/micro-shop-products/issues"
  },
  "homepage": "https://github.com/amitsatpute-pyjs/micro-shop-products#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "knex-stringcase": "^1.4.6",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "swagger-ui-express": "^4.6.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/knex": "^0.16.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.14.5",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  }
}
